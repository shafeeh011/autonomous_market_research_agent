# Contents of the file: /market-research-agent/market-research-agent/.env.example

# Example environment variables for the market research agent project
# Please rename this file to .env and fill in the required values

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# SerpAPI Key
SERPAPI_KEY=your_serpapi_key_here

# Database URL
DATABASE_URL=postgresql://user:password@localhost:5432/your_database_name

# ChromaDB or Pinecone API Key (if applicable)
VECTOR_DB_API_KEY=your_vector_db_api_key_here

# Other configuration variables
DEBUG_MODE=True
PORT=8501  # Port for Streamlit app
FASTAPI_PORT=8000  # Port for FastAPI server